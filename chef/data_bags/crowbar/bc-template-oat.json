{
  "id": "bc-template-oat",
  "description": "Installation for Oat",
  "attributes": {
    "oat": {
      "db": {
        "database": "oat_db",
        "user": "oat",
        "password": ""
      },
      "apache_listen_port": "8383",
      "pcr_select_mask": "FFFFFF",
      "oa_attest_interval": 60000,
      "oa_attest_timeout": 60000,
      "oaws_attest_timeout": 50000,
      "oaws_check_attest_interval": 1000,
      "alert_mail_to": "oatapp_admin@localhost",
      "alert_mail_from": "OATapp_no_reply",
      "alert_mail_user": "smtp_user",
      "alert_mail_pass": "smtp_pass",
      "alert_mail_port": 25,
      "alert_mail_host": "localhost",
      "mysql_instance": "none"
    }
  },
  "deployment": {
    "oat": {
      "crowbar-revision": 0,
      "element_states": {
        "oat-server": [ "readying", "ready", "applying" ],
        "oat-client": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
        [ "oat-server" ],
        [ "oat-client" ]
      ],
      "config": {
        "environment": "oat-base-config",
        "mode": "full",
        "transitions": false,
        "transition_list": [
        ]
      }
    }
  }
}

